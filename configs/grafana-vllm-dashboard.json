{
  "dashboard": {
    "id": null,
    "title": "vLLM Performance Dashboard",
    "tags": ["vllm", "llm", "performance"],
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Request Metrics",
        "type": "stat",
        "targets": [
          {
            "expr": "vllm_requests_total",
            "legendFormat": "Total Requests"
          },
          {
            "expr": "vllm_requests_active",
            "legendFormat": "Active Requests"
          },
          {
            "expr": "rate(vllm_requests_total[5m]) * 60",
            "legendFormat": "Requests/min"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "pluginVersion": "8.0.0"
      },
      {
        "id": 2,
        "title": "Response Time Percentiles",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, vllm_request_duration_seconds_bucket)",
            "legendFormat": "P50"
          },
          {
            "expr": "histogram_quantile(0.95, vllm_request_duration_seconds_bucket)",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.99, vllm_request_duration_seconds_bucket)",
            "legendFormat": "P99"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "options": {
          "tooltip": {
            "mode": "single",
            "sort": "none"
          },
          "legend": {
            "displayMode": "visible",
            "placement": "bottom"
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "min": 0
          }
        }
      },
      {
        "id": 3,
        "title": "Token Throughput",
        "type": "timeseries",
        "targets": [
          {
            "expr": "vllm_tokens_per_second",
            "legendFormat": "Tokens/sec"
          },
          {
            "expr": "rate(vllm_tokens_generated_total[5m])",
            "legendFormat": "Generated Rate"
          },
          {
            "expr": "rate(vllm_tokens_prompt_total[5m])",
            "legendFormat": "Prompt Rate"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8},
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          },
          "legend": {
            "displayMode": "visible",
            "placement": "bottom"
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0
          }
        }
      },
      {
        "id": 4,
        "title": "GPU Utilization",
        "type": "gauge",
        "targets": [
          {
            "expr": "vllm_gpu_utilization * 100",
            "legendFormat": "GPU Usage %"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 16},
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "red", "value": 90}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "GPU Memory Usage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "vllm_gpu_memory_used_bytes / 1024 / 1024 / 1024",
            "legendFormat": "Used (GB)"
          },
          {
            "expr": "vllm_gpu_memory_total_bytes / 1024 / 1024 / 1024",
            "legendFormat": "Total (GB)"
          },
          {
            "expr": "(vllm_gpu_memory_used_bytes / vllm_gpu_memory_total_bytes) * 100",
            "legendFormat": "Usage %"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 16},
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          },
          "legend": {
            "displayMode": "visible",
            "placement": "bottom"
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "decgbytes",
            "min": 0
          }
        }
      },
      {
        "id": 6,
        "title": "GPU Temperature",
        "type": "gauge",
        "targets": [
          {
            "expr": "vllm_gpu_temperature_celsius",
            "legendFormat": "Temperature Â°C"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 16},
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "celsius",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "red", "value": 85}
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Cache Performance",
        "type": "stat",
        "targets": [
          {
            "expr": "vllm_cache_hit_rate * 100",
            "legendFormat": "Hit Rate %"
          },
          {
            "expr": "vllm_cache_hits_total",
            "legendFormat": "Total Hits"
          },
          {
            "expr": "vllm_cache_misses_total",
            "legendFormat": "Total Misses"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 60},
                {"color": "green", "value": 80}
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "title": "Batch Processing Efficiency",
        "type": "timeseries",
        "targets": [
          {
            "expr": "vllm_batch_efficiency * 100",
            "legendFormat": "Efficiency %"
          },
          {
            "expr": "vllm_batch_size",
            "legendFormat": "Current Batch Size"
          },
          {
            "expr": "vllm_queued_requests",
            "legendFormat": "Queued Requests"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          },
          "legend": {
            "displayMode": "visible",
            "placement": "bottom"
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0
          }
        }
      },
      {
        "id": 9,
        "title": "System Memory Usage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "vllm_memory_usage_bytes / 1024 / 1024 / 1024",
            "legendFormat": "Memory Usage (GB)"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32},
        "options": {
          "tooltip": {
            "mode": "single",
            "sort": "none"
          },
          "legend": {
            "displayMode": "visible",
            "placement": "bottom"
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "decgbytes",
            "min": 0
          }
        }
      },
      {
        "id": 10,
        "title": "Request Rate Over Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(vllm_requests_total[1m]) * 60",
            "legendFormat": "Requests/min"
          },
          {
            "expr": "rate(vllm_requests_total[5m]) * 300",
            "legendFormat": "Requests/5min"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32},
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          },
          "legend": {
            "displayMode": "visible",
            "placement": "bottom"
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "min": 0
          }
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "instance",
          "type": "query",
          "query": "label_values(vllm_requests_total, instance)",
          "refresh": 1,
          "includeAll": true,
          "multi": true,
          "allValue": ".*",
          "current": {
            "selected": false,
            "text": "All",
            "value": "$__all"
          }
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "High Latency Alerts",
          "datasource": "prometheus",
          "enable": true,
          "expr": "vllm_request_duration_seconds > 30",
          "iconColor": "red",
          "textFormat": "High latency: {{$value}}s"
        }
      ]
    },
    "editable": true,
    "gnetId": null,
    "graphTooltip": 0,
    "hideControls": false,
    "links": [],
    "liveNow": false,
    "schemaVersion": 27,
    "style": "dark",
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "timezone": "",
    "uid": "vllm-performance",
    "version": 1,
    "weekStart": ""
  }
}